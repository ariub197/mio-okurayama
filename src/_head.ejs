
<%
include('/_utils');
// このブロックでは、このコンポーネントのdefault valueを設定します。
// include('/_head', {title: 'xxxxx'}) のような形でオプションを渡した時には、それが利用され、
// 渡されていない時には下記のデフォルト値が使われます。
/*
例）
titleに "default title" というデフォルト値が設定されていた場合、
include('/_head', {title: 'aaa'}) => 'aaa'
include('/_head', {title: ''}) => ''
include('/_head', {title: null}) => null
include('/_head', {}) => 'default title'
include('/_head', {title: undefined}) => 'default title'
*/

const params = ejsUtils.defaultParamsMapper(locals, {
  pageId: '',
  title: '',
  canonical: '',
  vendorScript: false,
  pageScript: '',
});

const common = {
  title: '公式｜ミオカステーロ大倉山（仮称）｜山田建設',
  description: '2駅2路線利用可能。東急東横線・急行停車駅「大倉山」駅 徒歩4分。東急新横浜線「新大倉山」駅徒歩7分。「ミオカステーロ大倉山（仮称）」誕生。',
  keywords: 'ミオカステーロ大倉山,ミオカステーロ大倉山（仮称）,ミオ大倉山,ミオ大倉山（仮称）,大倉山,東急東横線,東急新横浜線,山田建設,自在空間,新築マンション',
  url: 'https://www.yamada-kensetsu.co.jp/cms/bukken/o-kurayama/',
  themeColor: incJson.season_theme,
};
// 以下コードはtitleのフォーマットを変更するためのコードです。タイトルに（公式）などの文字が入らない場合は適宜処理を変更してください。
const offcialTtl = '公式｜';
const splitTtl = common.title.split(offcialTtl)[1];

const fmtTtl = params.pageId !== 'top' ? `${offcialTtl}${params.title}｜${splitTtl}` : `${offcialTtl}${splitTtl}`;
%>

<!DOCTYPE html>
<html lang="ja">
<head>
  <!-- meta -->
  <meta name="viewport" content="width=device-width, user-scalable=no" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <meta charset="UTF-8" />
  <meta name="format-detection" content="telephone=no" />
  <meta name="theme-color" content="<% if (common.themeColor ==='white') { %>#ffffff<% } %><% if (common.themeColor ==='black') { %>#000000<% } %>">
  <!-- links -->
  <link rel="SHORTCUT ICON" href="/cms/yk/img/favicon.ico" />

  <!-- font -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Cinzel&family=Licorice&family=Shippori+Mincho+B1:wght@400;700&display=swap" rel="stylesheet">
  <!-- title, description -->
  <title><%- fmtTtl %></title>
  <meta name="description" content="<%- common.description %>">
  <meta name="keywords" content="<%- common.keywords %>">
  <link rel="canonical" href="<%- common.url %><%- params.pageId !== 'top' ? params.canonical : '' ; %>">
  <meta property="og:title" content="<%- fmtTtl %>" />
  <meta property="og:type" content="<%- params.pageId === 'top' ? 'website' : 'article' %>"/>
  <meta property="og:url" content="<%- common.url %>" />
  <meta property="og:image" content="<%- common.url %>ogp/index.jpg" />
  <meta property="og:description" content="<%- common.description %>" />
  <!-- assets -->
  <link rel="stylesheet" href="<%= relativePath %>common/css/common.css" />
  <link rel="stylesheet" href="./css/style.css" /><% if (params.pageId == 'interactionPattern') { %>
  <script src="//maps.google.com/maps/api/js?key=AIzaSyBdQtnBRaQf1kiXDNIG-hmXGQOuWnt8TmQ&amp;language=ja"></script><% }%>
  <script src="<%= relativePath %>common/js/lib.js"></script>
  <% if (params.vendorScript) { %><!-- vendor -->
  <script src="<%= relativePath %>common/js/vendor/jquery-3.6.0.min.js"></script>
  <script src="<%- relativePath %>common/js/vendor/yall.min.js"></script>
  <script src="<%= relativePath %>common/js/utility.js"></script>
  <script src="<%= relativePath %>common/js/legacy-function.js"></script>
  <% }%>
  <% if (params.pageScript == 'brand' || params.pageScript == 'checkeyes') { %>
    <script src="<%= relativePath %>common/js/vendor/smoothscroll.js"></script>
    <script src="./js/function.js"></script>
  <% }%>
</head>

<!--cookie-->
<link href="/cms/yk/css/ga-cookie-opt-in.css" rel="stylesheet"> <script src="/cms/yk/js/ga-cookie-opt-in.js"></script>
<!--cookie-->
<body class="site-theme-<%- common.themeColor %>">
